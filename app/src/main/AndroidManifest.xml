<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.study.zenlyclone">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_ice_cream"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_ice_cream"
        android:supportsRtl="true"
        android:theme="@style/Theme.ZenlyClone">
        <activity android:name=".ConfirmOtpActivity"></activity>
        <activity android:name=".PhoneActivity"></activity>
        <activity
            android:name=".NameActivity"
            android:windowSoftInputMode="adjustResize|stateAlwaysVisible" />
        <activity android:name=".DobActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>